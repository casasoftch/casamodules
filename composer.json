{
    "name": "casasoft/casamodules",
    "type": "library",
    "description": "Casasoft Modules for Zend Framework 2 (PSR7) implementations.",
    "keywords": [
        "zf2"
    ],
    "homepage": "http://casasoft.ch/",
    "license": "GPL",
    "authors": [
        {
            "name": "Jens Stalder",
            "email": "jens.stalder@casasoft.ch"
        }
    ],
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0",
        "laminas/laminas-dependency-plugin": "2.6.0"
    },
    "require-dev": {},
    "suggest": {},
    "autoload": {
        "psr-0": {
            "CasasoftAuth\\": "src/",
            "CasasoftEmail\\": "src/",
            "CasasoftHelpers\\": "src/",
            "CasasoftThumb\\": "src/",
            "CasasoftStandards\\": "src/",
            "DbGenerator\\": "src/",
            "CasasoftMessenger\\": "src/",
            "CasasoftGeo\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }},
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
